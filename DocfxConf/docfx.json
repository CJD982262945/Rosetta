{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Rosetta.Runtime.csproj",
            "Rosetta.Editor.csproj"
          ],
          "src": ".."
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/toc.yml"
        ]
      },
      {
        "files": [
          "guides/**/*.{md,yml}",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "src": "..",
        "files": [
          "Res/**"
        ]
      },
      {
        "src": ".",
        "files": [
          "guides/res/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../Docs",
    "globalMetadata":{      
      "_appTitle": "Rosetta website",
      "_appFooter": "<span>Copyright Â© Shitake.<br>Generated by <strong>DocFX</strong></span>"
    },
    "fileMetadataFiles": [],
    "postProcessors": [ "ExtractSearchIndex" ],
    "markdownEngineName": "markdig",
    "template": [
      "default",
      "custom_template"
    ]
  }
}